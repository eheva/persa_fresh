{
  "packageManager": "pnpm@9.12.2",
  "name": "persa_fresh",
  "version": "1.0.8",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/payload-cloud": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/storage-s3": "3.69.0",
    "cross-env": "7.0.3",
    "graphql": "16.8.1",
    "next": "16.1.0",
    "payload": "3.69.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "0.32.6",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@types/node": "22.5.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "9.16.0",
    "eslint-config-next": "15.2.2",
    "prettier": "3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "tar-fs@>=2.0.0 <2.1.4": ">=2.1.4",
      "tar-fs@>=3.0.0 <3.1.1": ">=3.1.1",
      "tar-fs@>=3.0.0 <3.0.9": ">=3.0.9",
      "tar-fs@>=2.0.0 <2.1.3": ">=2.1.3",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "nodemailer@<=7.0.10": ">=7.0.11",
      "nodemailer@<7.0.11": ">=7.0.11",
      "@smithy/config-resolver@<4.4.0": ">=4.4.0"
    }
  }
}
